import type { CosmosTransaction, RawCosmosTransaction } from "./types";
export declare const serializeCosmosTransaction: ({ amount, recipient, family, mode, fees, gas, memo, }: CosmosTransaction) => RawCosmosTransaction;
export declare const deserializeCosmosTransaction: ({ amount, recipient, family, mode, fees, gas, memo, }: RawCosmosTransaction) => CosmosTransaction;
